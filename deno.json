{
  "name": "@andyb/env",
  "version": "0.0.1",
  "exports": "./mod.ts",
  "tasks": {
    "dev": "deno run --watch main.ts",
    "init:cache": "deno cache npm:husky npm:lint-staged",
    "init:husky": "deno run -A npm:husky",
    "init": "deno task init:cache && deno task init:husky",
    "lint-staged": "deno run -A npm:lint-staged"
  },
  "publish": {
    "include": ["src/**/*", "LICENSE.md", "README.md"],
    "exclude": ["src/**/*.test.*"]
  }
}
